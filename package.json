{
  "name": "markdown-url-validator",
  "version": "1.0.02",
  "description": "Lib for testing links in a markdown file",
  "main": "./cli.js",
  "bin": {
    "markdown-url-validator": "./cli.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielvbauer/markdown-url-validator"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "cli": "node cli.js ./arquivos/texto1.md"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "keywords": ["markdown", "links", "test", "analyzer", "checker", "URL", "url"],
  "author": "Gabriel Bauer Vieira <gabriel.bauer9@hotmail.com>",
  "bugs": {
    "url" : "https://github.com/gabrielvbauer/markdown-url-validator/issues",
    "email" : "gabriel.bauer9@hotmail.com"
  },
  "homepage": "https://github.com/gabrielvbauer/markdown-url-validator",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "chalk": "^4.1.2",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest": "^27.5.1"
  }
}
